#include <stdlib.h>
#include <stdio.h>
/**
 * exit_shell - Exits the shell cleanly.
 * @command: The execution command (to handle specific actions if necessary).
 */

void exit_shell(char *command)
{
    (void)command;  /* Le paramètre n'est pas utilisé ici */
    exit(0);  /* Quitter le shell avec un code de succès */
}
